{
  "name": "yedpay/yedpay-magento",
  "description": "A Magento 2 Extension to Integrate payment through Yedpay.",
  "require": {
    "php": ">=5.6",
    "magento/module-sales": "100.3.*",
    "magento/module-checkout": "100.3.*",
    "magento/module-payment": "100.3.*",
    "magento/framework": "100.3.*",
    "magento/magento-composer-installer": "100.3.*",
    "yedpay/php-library": "1.2.0",
    "ext-json": "*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/yedpay/php-library.git"
    }
  ],
  "type": "magento2-module",
  "version": "1.0.0",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Yedpay\\YedpayMagento\\": "",
      "Yedpay\\Client\\": "yedpay/php-library/src/Client.php"
    }
  }
}
